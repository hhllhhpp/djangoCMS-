#: models.py:195
msgid "google map"
msgstr "谷歌地图"

#custom
#: models.py:265
msgid "google map maker"
msgstr "谷歌地图生成器"

#custom
#: models.py:309
msgid "google map route"
msgstr "谷歌地图路由"